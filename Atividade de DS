class Filme1{
	String nome;
	int duracaoEmMinutos;
	double duracaoEmHoras;

		double exibirDuracaoEmHoras(){
			
			duracaoEmHoras= duracaoEmMinutos / 60;
			
			return duracaoEmHoras;	
	}
}


class Filme2{
	String nome;
	int duracaoEmMinutos;
